package Practice;

import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
public class Pattern {
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) {
		int i=0;
		int n=Pattern.sc.nextInt();
		for(i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if(j%2==0)	
				System.out.print("*");
				else
				System.out.println("#");
			}
			System.out.println(" ");
		}
		sc.close();
	}
}
