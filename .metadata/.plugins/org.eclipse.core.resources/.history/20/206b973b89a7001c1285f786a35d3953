package Practice;

/**
 * @Author -- TkGitcode
 */
public enum Activities1 {
	TAMIL("1.Tamil"), ENGLISH("2.English"), HINDI("3.Hindi");

	String Activities1;

	Activities1(String Activities1) {
		this.Activities1 = Activities1;
	}

	String getvalue() {
		return Activities1;
	}
}

enum Activities2 {
	கேள்வியைஎழுப்புங்கள், சலுகையைமாற்றவும், கஸ்டமர்கேர்உடன்இணைக்கவும்;

	String Activities2;

	String getvalue() {
		return Activities2;
	}
}

enum Activities3 {
	REQUESTFORSERVICE, CHANGEAOFFER, CONNECTWITHCUSTIMERCARE;

	String Activities3;

	String getvalue() {
		return Activities3;
	}
}

enum Activities4 {
	प्रश्नपूछें, एकप्रस्तावबदलें, कस्टमरकेयरसेजुड़ें;

	String Activities4;

	String getvalue() {
		return Activities4;
	}
}

enum Activities5 {
	YOURREQUESTWASSUCCESSFULLYSUBMITED, YOUROFFERISSUCCESSFULLYCHANGEED, THANKYOUFORCONNECTINGWITHAS;

	String Activities5;

	String getvalue() {
		return Activities5;
	}
}

உங்கள் கோரிக்கை வெற்றிகரமாக முழுமையாக சமர்ப்பிக்கப்பட்டது,உங்கள் சலுகை வெற்றிகரமாக மாற்றப்பட்டது,உடன் இணைத்ததற்கு நன்றி
