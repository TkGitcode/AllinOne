//						else
//						System.out.print("*");
//					}
//			}
//             else	
//			for(int j=0;j<n;j++)
//			{
//				System.out.print("*");
//			}
//			System.out.println();
//			}
//			else
//			{
//			  for(int j=0;j<n;j++)
//			  {
//				  if(j==n/2)
//				  System.out.print("#");
//				  else
//				  System.out.print("*");
//			  }
//			System.out.println(" ");
//			}
		}
		sc.close();
	}
}
