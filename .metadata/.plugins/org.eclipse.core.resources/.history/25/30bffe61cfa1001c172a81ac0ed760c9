public class Pattern {
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) {
		int n=Pattern.sc.nextInt();
		for(int i=0;i<n;i++)
		{
			
			
			if(i!=0 && i!=n-1)
			{
				for(int i=0;i<n;i++)
				System.out.print("*");
			}
			else
				System.out.print(" ");
			System.out.println();
//			if(i%2==0)
//			{
//				if(i==n/2)
//			{
