package Practice;
/** 
* @Author -- TkGitcode
*/
public class Pattern3 {

	public static void main(String[] args) {
		char[] c= {'A','B','C','D','E'};
		for(int i=0;i<c.length;i++)
		{
			for(int j=0;j<=i;j++)
			{
				System.out.print(c[j]+" ");
			}
			//if(i!=0)
			for(int j=i;j>=0;j--)
			{
				System.out.print(c[j]+" ");
			}
			System.out.println();
		}

	}

}
