package Practice;

import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
public class Codecheff {

	static int[] rem(long n)
	{
		//String k="";
		int j=0;
		String k2=String.valueOf(n);
		int k1[]=new int[k2.length()];
		while(n!=0)
		{
			int rem=(int) (n%10);
			k1[j++]=rem;
		}
		return k1;
	}
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		while(n-- >0)
		{
			long a=sc.nextInt();
			System.out.println(rem(a));
		}

	}

}
