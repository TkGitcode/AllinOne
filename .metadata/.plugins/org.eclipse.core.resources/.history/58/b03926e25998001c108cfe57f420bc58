import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
public class Viral_Advertising {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int mid,liked,com=0;
		mid=n/2;
		for(int i=0;i<n;i++)
		{
		   
		   liked=mid;
		   System.out.println("liked "+liked);
		   com+=liked;
		}
		System.out.println(com);

	}

}
