import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
public class Circular_Array_Rotation {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int k=sc.nextInt();
		int q=sc.nextInt();
		int a[]=new int[n];
		int b[]=new int[n];
		for(int i=0;i<n;i++)
		{
			a[i]=sc.nextInt();
		}
		for(int i=0;i<q;i++)
		{
			b[i]=sc.nextInt();
		}
		for(int i=0;i<k;i++)
		{
			for(int j=0;j<n-1;j++)
			{
				System.out.println(a[n-i]+" "+a[n-i-1]);
			}
			
		}

	}

}
