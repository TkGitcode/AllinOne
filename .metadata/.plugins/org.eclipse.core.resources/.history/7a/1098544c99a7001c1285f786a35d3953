package Practice;

import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
public class Codecheff {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
		    int a=sc.nextInt();
		    int rem1=a%10;
		    int rem2=a%5;
		    if(rem1==0)
		    {
		    	System.out.println(a/10);
		    }
		    else if(rem2==0)
		    {
		    	int val=a/10+rem1/5;
		    	System.out.println(-1);
		    }
		}

	}

}
