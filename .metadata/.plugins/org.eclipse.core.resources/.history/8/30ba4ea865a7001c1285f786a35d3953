public enum Activities1 {
	"1".TAMIL("1.Tamil"), ENGLISH("2.English"), HINDI("3.Hindi");

	String Activities1;

	Activities1(String Activities1) {
		this.Activities1 = Activities1;
	}

	String getvalue() {
		return Activities1;
	}
}

