import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
public class Circular_Array_Rotation {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int k=sc.nextInt();
		int q=sc.nextInt();
		int a[]=new int[n];
		int b[]=new int[n];
		int temp;
		for(int i=0;i<n;i++)
		{
			a[i]=sc.nextInt();
		}
		for(int i=0;i<q;i++)
		{
			b[i]=sc.nextInt();
		}
		for(int i=0;i<k;i++)
		{
			for(int j=1;j<n;j++)
			{
				System.out.println(a[n-j]+" "+a[n-j-1]);
				 temp=a[n-j];
				a[n-j]=a[n-j-1];
				
			}
			
		}

	}

}
