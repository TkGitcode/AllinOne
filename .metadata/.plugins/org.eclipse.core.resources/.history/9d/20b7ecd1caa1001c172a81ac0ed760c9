package Practice;

import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
public class Pattern {
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) {
		int i=0;
		int n=Pattern.sc.nextInt();
		for(i=0;i<n;i++)
		{
			if(i%2==0)
			{
			for(int j=0;j<n;j++)
			{	if(i==2 && j!=i-1 && j!=n)
				System.out.print("*");
			else
				System.out.print("#");
			}
			System.out.println(" ");
			}
			else
			{
			  for(int j=0;j<n;j++)
			  {
				  if(j==2)
				  System.out.print("#");
				  else
				  System.out.print("*");
			  }
			System.out.println(" ");
			}
		}
		sc.close();
	}
}
