package Java1;

import java.util.Scanner;

class PswMat
{
	 boolean pass(String y)
	{
		Scanner v = new Scanner(System.in);
		String l=v.nextLine();
		if(l.equals(y))
		{
			v.close();
			return true;	
		}
		else
		{
			v.close();
			return false;
		}
		
	}
	 
}
 class randomstring {

	public static void main(String[] args) {
		String l= "ABCDEFGHIJKLMNOPQRSTUVWXYZ12345";
        String Pswt = "";
        PswMat PM= new PswMat();
        for(int i=0;i<10;i++)
        {
        	Pswt += l.charAt((int)(Math.random() *  l.length()));
        }
        boolean b= PM.pass(Pswt);
        if(b)
        {
        	System.out.print("You Have Entered Correct PassWord");
        }
        else
        	System.out.print("Wrong Pass");
        System.out.print(Pswt);
	}

}
