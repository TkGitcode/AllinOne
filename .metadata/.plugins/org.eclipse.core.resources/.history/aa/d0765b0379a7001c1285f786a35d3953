public enum Activities1 {
	TAMIL("1.Tamil"), ENGLISH("2.English"), HINDI("3.Hindi");

	String Activities1;

	Activities1(String Activities1) {
		this.Activities1 = Activities1;
	}

	String getvalue() {
		return Activities1;
	}
}

enum Activities2 {
	REQUESTFORSERVICE("1.Tamil"), RAISEQUERY("2.English"), CONNECTWITHCUSTIMERCARE("3.Hindi");

	String Activities2;

	Activities2(String Activities2) {
		this.Activities2 = Activities2;
	}

	String getvalue() {
		return Activities2;
	}
}
enum Activities3 {
	கேள்வியைஎழுப்புங்கள், சேவைக்கானகோரிக்கை("2.English"), CONNECTWITHCUSTIMERCARE("3.Hindi");

	String Activities2;

	Activities2(String Activities2) {
		this.Activities2 = Activities2;
	}

	String getvalue() {
		return Activities2;
	}
}





