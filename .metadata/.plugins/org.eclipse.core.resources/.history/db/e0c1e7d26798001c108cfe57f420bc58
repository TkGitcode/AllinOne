import java.util.Scanner;

/** 
* @Author -- TkGitcode
*/
class Retpos
{
	int a[];
	int n=0,j=0;
	Retpos(int b[],int n)
	{
		a=b;
	}
	int Tofin()
	{
	j=0;
	for(int i=1;i<n;i++)
	{
		if(a[j++]==i)
		{
			break;
		}
	}
	return j;
	}
}
public class Sequence_Equation {

	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a[]=new int[100];
		for(int i=0;i<n;i++)
		{
			a[i]=sc.nextInt();
		}
		

	}

}
