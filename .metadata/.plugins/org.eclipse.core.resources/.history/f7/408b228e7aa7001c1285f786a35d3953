public enum Activities1 {
	TAMIL("1.Tamil"), ENGLISH("2.English"), HINDI("3.Hindi");

	String Activities1;

	Activities1(String Activities1) {
		this.Activities1 = Activities1;
	}

	String getvalue() {
		return Activities1;
	}
}


enum Activities2 {
	கேள்வியைஎழுப்புங்கள், சேவைக்கானகோரிக்கை, கஸ்டமர்கேர்உடன்இணைக்கவும்;

	String Activities2;
	String getvalue() {
		return Activities2;
	}
}
